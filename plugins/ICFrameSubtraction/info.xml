<?xml version="1.0" encoding="utf-8" ?>
<VideoAnalysis>
    <Title>Imagem Cinemática - Frame Subtraction</Title>
    <Author>Imagem Cinemática</Author>
    <Description>Applies frame subtraction to detect movement.</Description>
    <Version>1.0.0</Version>
    <FilterPage in="BGR" out="GRAY" name="Pre-Filter">
        <Filter name="ICBGR2GRAY" />
        <Filter name="ICGrayBlurFilter" />
    </FilterPage>
    <FilterPage in="GRAY" out="GRAY" name="Threshold">
        <Filter name="ICGrayBlurFilter" />
        <Filter name="ICAdaptiveThreshold" />
    </FilterPage>

    <FilterPage in="GRAY" out="GRAY" name="Post-Filter">
        <Filter name="ICGrayBlurFilter" />
    </FilterPage>

    <FilterPage in="GRAY" out="GRAY" name="Dilatation">
        <Filter name="ICDilateFilter" />
    </FilterPage>
    
</VideoAnalysis>
